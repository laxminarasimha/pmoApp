var express = require("express"),
 router = express.Router(),
 resourceMappingDAO = require("../models/resourceMappingDAO.js");
 
router.post("/", function(req, res) {
var obj = req.body;
resourceMappingDAO.find({"mappedResource.resourcename": obj.resource,"region":     obj.region,"resourceType": obj.resourceType}, function(err, data) {
 if (err) {
 res.send("error");
 return;
 }
 res.send(data);
 });
});
 
module.exports = router;


